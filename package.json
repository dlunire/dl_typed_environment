{
  "name": "dlunire-envtype",
  "displayName": "DL Typed Environment (Rebuild)",
  "description": "Variables con definici√≥n de tipo",
  "version": "0.0.8",
  "repository": {
    "url": "https://github.com/dlunire/dl_typed_environment"
  },
  "icon": "images/env.png",
  "publisher": "dlunire",
  "main": "out/extension.js",
  "activationEvents": ["onLanguage:dlunire-envtype"],
  "engines": {
    "vscode": "^1.82.0",
    "code-insiders": "^1.82.0",
    "vscodium": "^1.82.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "dlunire-envtype",
        "aliases": [
          "DL Typed Environment",
          "dlunire-envtype"
        ],
        "extensions": [
          ".env.type",
          ".env.type.example",
          ".type",
          ".type.example"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "dlunire-envtype",
        "scopeName": "source.env.type",
        "path": "./syntaxes/envtype.tmLanguage.json"
      }
    ]
  },
  "devDependencies": {
    "typescript": "^5.9.3",
    "vscode": "^1.1.34"
  },
  "scripts": {
    "vscode:prepublish": "tsc",
    "build": "tsc -p .",
    "watch": "tsc -watch"
  }
}